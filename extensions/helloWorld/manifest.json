{
    "manifest_version": 3,
    "name": "Hello World Extension",
    "version": "1.0.0",
  
    "permissions": [
      "activeTab"
    ],
  
    "browser_extension": {
      "browser_action": {
        "default_icon": "icon.png",
        "default_title": "Hello World Extension",
        "default_popup": "popup.html"
      }
    }
  }
  